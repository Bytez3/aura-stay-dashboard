# AURA Dashboard Environment Fix
# Add this to your ~/.zshrc to permanently fix Node.js version issues

unset npm_config_prefix
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"

# Auto-switch to Node.js 20.18.0 in this project
if [[ "$PWD" == *"aura-stay-dashboard"* ]]; then
  nvm use 20.18.0 2>/dev/null || nvm install 20.18.0
fi 